{
  "name": "clientlambda",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build-local": "npx esbuild run_local.ts --bundle --outfile=out.js --platform=node",
    "predev": "npm run inject",
    "dev": "npx nodemon --exec 'npm run build-local ; dotenvx run -f variables.env -f injected.credentials.env -- node out.js' --ext 'ts,env' --ignore 'lib/' --ignore 'bin/'",
    "postdev": "npm run rminject",
    "inject": "op.exe inject -f -i ../../../environment/credentials.env -o injected.credentials.env",
    "rminject": "rm injected.*.env"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.577.0",
    "@codegenie/serverless-express": "^4.14.1",
    "ejs": "^3.1.10",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.12"
  }
}
