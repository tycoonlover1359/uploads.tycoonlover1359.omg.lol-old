{
  "name": "tycoonlover1359-pics",
  "version": "0.1.0",
  "bin": {
    "tycoonlover1359-pics": "bin/tycoonlover1359-pics.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "build-local": "npx esbuild src/run_local.ts --bundle --outfile=out.js --platform=node",
    "dev": "npx nodemon --exec 'npm run build-local ; node out.js' --ext 'ts' --ignore 'lib/' --ignore 'bin/'",
    "inject": "op.exe inject -f -i credentials.env -o injected.credentials.env",
    "rminject": "rm injected.*.env",
    "cdk": "npm run inject ; dotenvx run -f injected.credentials.env -- npx cdk",
    "postcdk": "npm run rminject"
  },
  "devDependencies": {
    "@types/node": "20.12.7",
    "aws-cdk": "2.141.0",
    "esbuild": "^0.21.1",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.4.5"
  },
  "dependencies": {
    "@codegenie/serverless-express": "^4.14.1",
    "@dotenvx/dotenvx": "^0.38.0",
    "@types/express": "^4.17.21",
    "aws-cdk-lib": "2.141.0",
    "constructs": "^10.0.0",
    "express": "^4.19.2",
    "source-map-support": "^0.5.21"
  }
}
